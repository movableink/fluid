  "scripts": {
    "build": "ember build",
    "lint-hbs": "ember-template-lint .",
    "lint-js": "eslint .",
    "start": "ember server",
    "test": "ember exam",
    "ci-test": "EMBER_ENV=test QUNIT_CONSOLE_GROUPER=false PERCY_TOKEN=$PERCY_TOKEN percy exec -- ember exam",
